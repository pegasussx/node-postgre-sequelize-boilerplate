{
  "name": "node-express-sequelize-postgre",
  "version": "1.0.0",
  "description": "A boilerplate for node-express-sequelize-postgre",
  "main": "server.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env NODE_ENV=development DEBUG=node-server:* nodemon ./bin/www",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:down": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "6.0.0",
    "express-validation": "3.0.8",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-watch": "^8.0.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0"
  }
}
